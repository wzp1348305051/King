<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="InputView">
        <attr name="input_icon" format="string" />
        <attr name="input_hint" format="string" />
    </declare-styleable>

    <declare-styleable name="HeaderView">
        <attr name="header_leftText" format="string" />
        <attr name="header_leftSize" format="dimension" />
        <attr name="header_leftColor" format="color" />
        <attr name="header_middleText" format="string" />
        <attr name="header_middleSize" format="dimension" />
        <attr name="header_middleColor" format="color" />
        <attr name="header_rightText" format="string" />
        <attr name="header_rightSize" format="dimension" />
        <attr name="header_rightColor" format="color" />
    </declare-styleable>

    <declare-styleable name="EmptyView">
        <attr name="empty_text" format="string" />
    </declare-styleable>
</resources>